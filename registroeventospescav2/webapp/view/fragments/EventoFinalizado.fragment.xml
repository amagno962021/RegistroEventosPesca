<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:form="sap.ui.layout.form">
	<Dialog title="Evento registrado">
		<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Text maxLines="1" text="{DetalleMarea>/Utils/MensajeGrabacion}"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement>
								<HBox>
									<Link
										emphasized="false"
										enabled="true"
										href=""
										subtle="false"
										target=""
										text="empty string"
										textAlign="Initial"
										textDirection="Inherit"
										validateUrl="false"
										width=""
										wrapping="false"
								>
															
								</Link>
									<Text
										renderWhitespace="false"
										text="empty string"
										textAlign="Begin"
										textDirection="Inherit"
										width=""
										wrapping="true"
										wrappingType="Normal"
									>
									</Text>
								</HBox>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
		</content>
		<beginButton>
			<Button type="Emphasized" id="btnConfirmSave" text="Aceptar" press="onCloseConfirm" enabled="true"></Button>
		</beginButton>
		<endButton>
			<Button text="Cancelar" press=".onCancelConfirm"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>