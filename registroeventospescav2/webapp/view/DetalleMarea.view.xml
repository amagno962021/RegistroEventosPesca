<mvc:View
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:layout="sap.ui.layout"
	xmlns:form="sap.ui.layout.form"
	controllerName="com.tasa.registroeventospescav2.controller.DetalleMarea" displayBlock="true"
	height="100%">
	<!-- <App> removed -->
	<!--	<pages> removed -->
	<Page title="Ingresar datos" showNavButton="true" navButtonPress="onBackListMarea" showFooter="true">
		<content>
			<layout:VerticalLayout width="100%" visible="true">
				<layout:content>
					<ObjectHeader intro="{DetalleMarea>/Cabecera/DESC_CDMMA}" title="{DetalleMarea>/Cabecera/NRMAR}" icon="sap-icon://product" number="" numberUnit="" numberState="None" titleActive="false"
								backgroundDesign="Translucent" condensed="false" visible="true">
						<attributes>
							<ObjectAttribute title="Embarcacion" text="{DetalleMarea>/Cabecera/CDEMB} {DetalleMarea>/Cabecera/NMEMB}" active="false" visible="true"/>
							<ObjectAttribute title="Matricula" text="{DetalleMarea>/Cabecera/MREMB}" active="false" visible="true"/>
							<ObjectAttribute title="Armador" text="{DetalleMarea>/Cabecera/CDEMP} {DetalleMarea>/Cabecera/NAME1}" active="false" visible="true"/>
							<ObjectAttribute text="{DetalleMarea>/Cabecera/TXTNOTIF}" active="false" visible="true"/>
							<ObjectAttribute text="{DetalleMarea>/Cabecera/TXTNOTIF1}" active="false" visible="true"/>
						</attributes>
						<statuses>
							<ObjectStatus text="Abierto" state="Success" icon="sap-icon://status-completed" visible="true"/>
						</statuses>
						<markers/>
					</ObjectHeader>
				</layout:content>
			</layout:VerticalLayout>
			<IconTabBar expandable="false" expanded="true" applyContentPadding="false" backgroundDesign="Solid" upperCase="false" visible="true"
						selectedKey="Ok" select="onSelectTabMarea" id="itbDetalleMarea">
				<items>
					<IconTabFilter key="itfDatosGenerales" iconColor="Default" text="Datos Generales" count="" design="Vertical" showAll="false" textDirection="Inherit" enabled="true"
								visible="true" iconDensityAware="false">
						<content>
							<form:Form width="100%" editable="true" visible="true">
								<form:title>
									<core:Title text="Observación" level="Auto"/>
								</form:title>
								<form:formContainers>
									<form:FormContainer visible="true">
										<form:formElements>
											<form:FormElement visible="true">
												<form:label>
													<Label text="Observación" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
												</form:label>
												<form:fields>
													<Input value="{DetalleMarea>/Cabecera/OBMAR}"  enabled="true"  width="27.5%"
																valueStateText="Invalid entry"/>
												</form:fields>
											</form:FormElement>	
										</form:formElements>
										<form:title/>
									</form:FormContainer>
								</form:formContainers>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
							<form:Form width="100%" editable="true" visible="true">
								<form:title>
									<core:Title text="Pertenencia" level="Auto"/>
								</form:title>
								<form:formContainers>
									<form:FormContainer visible="true">
										<form:formElements>
											<form:FormElement visible="true">
												<form:label>
													<Label text="Embarcacion" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
												</form:label>
												<form:fields>
													<Input value="{DetalleMarea>/DatosGenerales/CDEMB}"  description="{DetalleMarea>/DatosGenerales/NMEMB}"  enabled="false"  width="27.5%"
																 valueStateText="Invalid entry"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleArmadorComercial}">
												<form:label>
													<Label text="Armador Comercial" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
												</form:label>
												<form:fields>
													<Input value="{DetalleMarea>/DatosGenerales/CDEMP}" description="{DetalleMarea>/DatosGenerales/NAME1}" enabled="false" 
																width="60%" valueStateText="Invalid entry"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleLinkCrearArmador}">
												<form:label>
													<Label text=""/>
												</form:label>
												<form:fields>
													<Link text="Crear Armador" press="onCrearArmador" />
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleArmadorRuc}">
												<form:label>
													<Label text="RUC"/>
												</form:label>
												<form:fields>
													<Input value="{DetalleMarea>/DatosGenerales/NuevoArmador/RUC}" width="27.5%"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleArmadorRazon}">
												<form:label>
													<Label text="Razón Social"/>
												</form:label>
												<form:fields>
													<Input value="{DetalleMarea>/DatosGenerales/NuevoArmador/RAZON}" width="27.5%"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleArmadorCalle}">
												<form:label>
													<Label text="Calle"/>
												</form:label>
												<form:fields>
													<Input value="{DetalleMarea>/DatosGenerales/NuevoArmador/CALLE}" width="27.5%"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleArmadorDistrito}">
												<form:label>
													<Label text="Distrito"/>
												</form:label>
												<form:fields>
													<Input value="{DetalleMarea>/DatosGenerales/NuevoArmador/DISTRITO}" width="27.5%"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleArmadorProvincia}">
												<form:label>
													<Label text="Provincia"/>
												</form:label>
												<form:fields>
													<Input value="{DetalleMarea>/DatosGenerales/NuevoArmador/PROVINCIA}" width="27.5%"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleArmadorDepartamento}">
												<form:label>
													<Label text="Departamento"/>
												</form:label>
												<form:fields>
													<ComboBox selectedKey="{DetalleMarea>/DatosGenerales/NuevoArmador/DEPARTAMENTO}" items="{DetalleMarea>/Config/datosCombo/Departamentos}" width="27.5%">
														<core:Item key="{DetalleMarea>BLAND}" text="{DetalleMarea>BEZEI}" />
													</ComboBox>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleLinkSelecArmador}">
												<form:label>
													<Label text=""/>
												</form:label>
												<form:fields>
													<Link text="Seleccionar Armador" press="onSeleccionarArmador" />
												</form:fields>
											</form:FormElement>
											<form:FormElement>
												<form:label>
													<Label text="Sistema de Pesca" width="100%"/>
												</form:label>
												<form:fields>
													<Text text="{DetalleMarea>/DatosGenerales/CDSPE} {DetalleMarea>/DatosGenerales/DSSPE}" width="auto" maxLines="1" wrapping="false"/>
												</form:fields>
											</form:FormElement>
										</form:formElements>
										<form:title/>
									</form:FormContainer>
								</form:formContainers>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
							<form:Form width="100%" editable="true" >
								<form:title>
									<core:Title text="Travesia" level="Auto"/>
								</form:title>
								<form:formContainers>
									<form:FormContainer>
										<form:formElements>
											<form:FormElement visible="{DetalleMarea>/Config/visibleMotMarea}">
												<form:label>
													<Label text="Motivo de Marea" width="100%" />
												</form:label>
												<form:fields>
													<ComboBox selectedKey="{DetalleMarea>/DatosGenerales/CDMMA}" items="{DetalleMarea>/Config/datosCombo/MotivosMarea}" width="27.5%" change="onValidaMotivo" >
														<core:Item key="{DetalleMarea>id}" text="{DetalleMarea>descripcion}" />
													</ComboBox>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleUbiPesca}">
												<form:label>
													<Label text="Ubicación de Pesca"  width="100%" />
												</form:label>
												<form:fields>
													<ComboBox selectedKey="{DetalleMarea>/DatosGenerales/INUBC}" items="{DetalleMarea>/Config/datosCombo/UbicPesca}" width="27.5%">
														<core:Item key="{DetalleMarea>id}" text="{DetalleMarea>descripcion}" />
													</ComboBox>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleFecHoEta}">
												<form:label>
													<Label text="Fecha/Hora ETA." width="100%" />
												</form:label>
												<form:fields>
													<HBox>
														<DatePicker value="{DetalleMarea>/DatosGenerales/FEARR}" width="90%"  valueStateText="Invalid entry" 
																 	valueFormat="dd/MM/yyyy HH:mm" displayFormat="dd/MM/yyyy HH:mm"/>
														<TimePicker id="TP2" value="{DetalleMarea>/DatosGenerales/HEARR}" valueFormat="HH:mm" displayFormat="HH:mm" width="90%"/>
													</HBox>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleFechIni}">
												<form:label>
													<Label text="Fecha de Inicio" width="100%" />
												</form:label>
												<form:fields>
													<HBox>
														<DatePicker value="{DetalleMarea>/DatosGenerales/FIMAR}" width="90%"  valueStateText="Invalid entry" 
																	valueFormat="dd/MM/yyyy" displayFormat="dd/MM/yyyy" enabled="{DetalleMarea>/Config/readOnlyFechIni}" />
														<TimePicker id="TP1" value="{DetalleMarea>/DatosGenerales/HIMAR}" valueFormat="HH:mm" displayFormat="HH:mm" width="90%" enabled="{DetalleMarea>/Config/readOnlyFechIni}"/>
													</HBox>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleFechFin}">
												<form:label>
													<Label text="Fecha de Fin" width="100%" />
												</form:label>
												<form:fields>
													<HBox>
														<DatePicker value="{DetalleMarea>/DatosGenerales/FFMAR}" width="90%"  valueStateText="Invalid entry" 
																 valueFormat="dd/MM/yyyy HH:mm" displayFormat="dd/MM/yyyy HH:mm"/>
														<TimePicker id="TP3" value="{DetalleMarea>/DatosGenerales/HFMAR}" valueFormat="HH:mm" displayFormat="HH:mm" width="90%"/>
													</HBox>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="{DetalleMarea>/Config/visibleEstMarea}">
												<form:label>
													<Label text="Estado de Marea" width="100%"/>
												</form:label>
												<form:fields>
													<ComboBox width="27.5%" selectedKey="{DetalleMarea>/DatosGenerales/ESMAR}" items="{DetalleMarea>/Config/datosCombo/EstMar}" enabled="{DetalleMarea>/Config/readOnlyEstaMar}">
														<items>
															<core:Item key="{DetalleMarea>id}" text="{DetalleMarea>descripcion}" />
														</items>
													</ComboBox>
												</form:fields>
											</form:FormElement>
										</form:formElements>
										<form:title/>
									</form:FormContainer>
								</form:formContainers>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
						</content>
					</IconTabFilter>
					<IconTabSeparator icon="" iconDensityAware="false"/>
					<IconTabFilter key="itfEventos" iconColor="Default" text="Eventos" count="" design="Vertical" showAll="false" textDirection="Inherit" enabled="true"
								visible="true" iconDensityAware="false">
						<content>
							<Table id="tblEventos" width="auto" noDataText="No data" mode="SingleSelectLeft"  growing="true" growingThreshold="20"
										growingScrollToLoad="true" class="sapUiResponsiveMargin" items="{path:'DetalleMarea>/Eventos/Lista', templateShareable:true}">
								<infoToolbar>
									<Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
										<content>
											<Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										</content>
									</Toolbar>
								</infoToolbar>
								<headerToolbar>
									<OverflowToolbar width="auto" height="auto" design="Transparent" visible="true" enabled="true">
										<content>
											<Title text="{DetalleMarea>/Eventos/TituloEventos}" titleStyle="Auto" width="auto" textAlign="Begin" visible="true" wrapping="false"/>
											<ToolbarSpacer width=""/>
											<OverflowToolbarButton text="Add" type="Default" icon="sap-icon://add" iconFirst="true" width="auto" enabled="true" visible="true"
														iconDensityAware="false" press="abrirCrearEvento"/>
											<OverflowToolbarButton text="Delete" type="Default" icon="sap-icon://delete" iconFirst="true" width="auto" enabled="true" visible="true"
														iconDensityAware="false" press="onEliminarEvento"/>
										</content>
									</OverflowToolbar>
								</headerToolbar>
								<columns>
									<Column width="5%" hAlign="Center" vAlign="Middle" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<header>
											<Text text="" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										</header>
										<footer/>
									</Column>
									<Column width="auto" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<header>
											<Text text="Evento" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										</header>
										<footer/>
									</Column>
									<Column width="auto" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<header>
											<Text text="Fecha/Hora Ini." width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										</header>
										<footer/>
									</Column>
									<Column width="auto" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<header>
											<Text text="Fecha/Hora Fin" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										</header>
										<footer/>
									</Column>
									<Column width="auto" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<header>
											<Text text="Zona de Pesca" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										</header>
										<footer/>
									</Column>
									<Column width="auto" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<header>
											<Text text="Puerto" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										</header>
										<footer/>
									</Column>
									<Column width="auto" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<header>
											<Text text="Planta" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										</header>
										<footer/>
									</Column>
									<Column width="auto" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<header>
											<Text text="Pesc. Decl" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										</header>
										<footer/>
									</Column>
									<Column width="auto" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<header>
											<Text text="Pesc Desc." width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										</header>
										<footer/>
									</Column>
									<Column width="auto" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<header>
											<Text text="" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
										</header>
										<footer/>
									</Column>
								</columns>
								<items>
									<ColumnListItem >
										<cells>
											<Text text="{DetalleMarea>NREVN}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
											<ObjectIdentifier title="{DetalleMarea>DESC_CDTEV}" text="" titleActive="false" visible="true"/>
											<Text text="{DetalleMarea>FECHOINI}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
											<Text text="{DetalleMarea>FECHOFIN}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
											<ObjectIdentifier title="{DetalleMarea>DSZPC}" text="{DetalleMarea>CDZPC}" titleActive="false" visible="true"/>
											<ObjectIdentifier title="{DetalleMarea>DSPTO}" text="{DetalleMarea>CDPTO}" titleActive="false" visible="true"/>
											<ObjectIdentifier title="{DetalleMarea>DESCR}" text="{DetalleMarea>CDPTA}" titleActive="false" visible="true"/>
											<ObjectNumber number="{DetalleMarea>CNPDC}" unit="Tn." emphasized="true" state="None"/>
											<ObjectNumber number="{DetalleMarea>CNPDS}" unit="Tn." emphasized="true" state="None"/>
											<Link
														text="Detalle"
														press="onNavEventos"
														width="auto"
														wrapping="false"
														textAlign="Begin"
														textDirection="Inherit"
														visible="true"
													/>
										</cells>
									</ColumnListItem>
								</items>
							</Table>
						</content>
					</IconTabFilter>
					<IconTabSeparator icon="" iconDensityAware="false"/>
					<IconTabFilter key="itfResComb" iconColor="Default" text="Reserva Combustible" count=""  visible="{DetalleMarea>/Config/visibleTabReserva}" iconDensityAware="false">
						<content>
							<form:Form width="100%" editable="true" visible="true">
								<form:title/>
