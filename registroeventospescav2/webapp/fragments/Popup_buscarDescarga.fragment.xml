<core:FragmentDefinition xmlns:core="sap.ui.core"  xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:layout="sap.ui.layout" xmlns:f="sap.f" xmlns:mvc="sap.ui.core.mvc" xmlns:tnt="sap.tnt" xmlns:w="sap.ui.integration.widgets">
    <Dialog contentWidth="100rem" horizontalScrolling="true" showHeader="true" state="None" stretch="false" title="Búsqueda de descarga" type="Standard" verticalScrolling="true">
        <content>
            <form:Form editable="true" visible="true">
                <form:title/>
                <form:formContainers>
                    <form:FormContainer visible="true">
                        <form:formElements>
                            <form:FormElement visible="true">
                                <form:label>
                                    <Label text="Nro. descarga" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </form:label>
                                <form:fields>
                                    <Input id="pbd_nro_descarga" type="Text" showValueHelp="false" enabled="true" visible="true" width="50%" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0"/>
                                </form:fields>
                            </form:FormElement>
                            <form:FormElement visible="true">
                                <form:label>
                                    <Label text="Tipo de pesca" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </form:label>
                                <form:fields>
                                    <ComboBox id="pbd_tipo_pesca" selectedKey="{popup_descarga>/TipoPescaSel}" items="{popup_descarga>/TipoPesca}" editable="true" enabled="true"  visible="true" width="50%" valueState="None" maxWidth="100%">
                                        <items>
                                            <core:Item key="{popup_descarga>id}" text="{popup_descarga>descripcion}" />
                                        </items>
                                    </ComboBox>
                                </form:fields>
                            </form:FormElement>
                            <form:FormElement visible="true">
                                <form:label>
                                    <Label text="Cod. Embarcación" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </form:label>
                                <form:fields>
                                    <Input id="pbd_cod_embarcacion" type="Text" showValueHelp="false" value="{popup_descarga>/CodEmb}" enabled="true" visible="true" width="50%" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0"/>
                                </form:fields>
                            </form:FormElement>
                            <form:FormElement visible="true">
                                <form:label>
                                    <Label text="Matrícula" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </form:label>
                                <form:fields>
                                    <Input id="pbd_matricula" type="Text" showValueHelp="false" value="{popup_descarga>/Matricula}" enabled="true" visible="true" width="50%" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0"/>
                                </form:fields>
                            </form:FormElement>
                            <form:FormElement visible="true">
                                <form:label>
                                    <Label text="Nom. embarcacion" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </form:label>
                                <form:fields>
                                    <Input id="pbd_nom_embarcacion" type="Text" showValueHelp="false" value="{popup_descarga>/NomEmb}" enabled="true" visible="true" width="50%" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0"/>
                                </form:fields>
                            </form:FormElement>
                        </form:formElements>
                        <form:title/>
                        
                    </form:FormContainer>
                    <form:FormContainer visible="true">
                        <form:formElements>
                            <form:FormElement visible="true">
                                <form:label>
                                    <Label text="Cod. planta" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </form:label>
                                <form:fields>
                                    <Input id="pbd_cod_planta" type="Text" showValueHelp="false" value="{popup_descarga>/CodPlanta}" enabled="true" visible="true" width="50%" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0"/>
                                </form:fields>
                            </form:FormElement>
                            <form:FormElement visible="true">
                                <form:label>
                                    <Label text="Nomb. planta" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </form:label>
                                <form:fields>
                                    <Input id="pbd_nom_planta" type="Text" showValueHelp="false" value="{popup_descarga>/NomPlanta}" enabled="true" visible="true" width="50%" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="0"/>
                                </form:fields>
                            </form:FormElement>
                            <form:FormElement visible="true">
                                <form:label>
                                    <Label text="Fecha inicio" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </form:label>
                                <form:fields>
                                    <DatePicker id="pbd_fecha_inicio" value="{popup_descarga>/FechaInicio}" enabled="true" width="50%" displayFormat="dd/MM/yyyy" required="false" valueStateText="Invalid entry" visible="true" valueFormat="dd/MM/yyyy" />
                                </form:fields>
                            </form:FormElement>
                            <form:FormElement visible="true">
                                <form:label>
                                    <Label text="Hora inicio" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </form:label>
                                <form:fields>
                                    <TimePicker id="pbd_hora_inicio" value="{popup_descarga>/HoraInicio}" valueFormat="HH:mm" displayFormat="HH:mm" width="50%"/>
                                </form:fields>
                            </form:FormElement>
                            <form:FormElement visible="true">
                                <form:label>
                                    <Label text="Estado" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                </form:label>
                                <form:fields>
                                    <ComboBox id="pbd_estado" editable="true" enabled="true" selectedKey="{popup_descarga>/Estado}" items="{popup_descarga>/ListaEstado}" visible="true" width="50%" valueState="None" maxWidth="100%">
                                        <items>
                                            <core:Item key="{popup_descarga>id}" text="{popup_descarga>descripcion}" />
                                        </items>
                                    </ComboBox>
                                </form:fields>
                            </form:FormElement>
                            <form:FormElement visible="true">
                                    <form:label>
                                        <Label text="" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
                                    </form:label>
                                    <form:fields>
                                        <Button text="Buscar" type="Default" icon="sap-icon://search" iconFirst="true" width="50%" enabled="true" visible="true" iconDensityAware="false" press=".consultarDescarga"/>
                                    </form:fields>
                            </form:FormElement>
                        </form:formElements>
                        <form:title/>
                        
                    </form:FormContainer>
                    
                </form:formContainers>
                
                <form:layout>
                    <form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="2" labelSpanM="2" labelSpanS="12"/>
                </form:layout>
            </form:Form>
            <Panel
                accessibleRole="Form"
                backgroundDesign="Translucent"
                expandAnimation="true"
                expandable="false"
                expanded="false"
                headerText="Detalle descarga"
                height="auto"
                width="100%"
            >
                <content>
                    <HeaderContainer id="headerContainer" scrollStep="200" showDividers="false">
                                    <HBox class="sapUiSmallMarginEnd sapUiSmallMarginBottom">
                                        <Avatar src="./image/descarga.png" class="sapUiMediumMarginEnd sapUiSmallMarginBottom" displaySize="L"/>

                                        <VBox class="sapUiSmallMarginBottom">
                                            <Title class="sapUiTinyMarginBottom">
                                                <Link text="{popup_descarga>/det_Descarga}"/>
                                            </Title>

                                            <HBox class="sapUiTinyMarginBottom" renderType="Bare">
                                                <Label text="Ticket:" class="sapUiTinyMarginEnd"/>
                                                <tnt:InfoLabel id="il10" text="{popup_descarga>/det_Ticket}" renderMode="Narrow" colorScheme="8"/>
                                            </HBox>

                                            <HBox class="sapUiTinyMarginBottom" renderType="Bare">
                                                <Label text="Tipo de pesca:" class="sapUiTinyMarginEnd"/>
                                                <tnt:InfoLabel id="il11" text="{popup_descarga>/det_TipoPesca}" renderMode="Narrow" colorScheme="8" />
                                            </HBox>

                                            <HBox>
                                                <Label text="Planta:" class="sapUiTinyMarginEnd"/>
                                                <tnt:InfoLabel id="il12" text="{popup_descarga>/det_Planta}" renderMode="Narrow" colorScheme="8" />
                                            </HBox>
                                        </VBox>
                                    </HBox>


                                    <VBox class="sapUiSmallMarginEnd sapUiSmallMarginBottom">
                                        <HBox class="sapUiTinyMarginBottom">
                                            <Label text="Nom. Planta:" class="sapUiTinyMarginEnd"/>
                                            <tnt:InfoLabel text="{popup_descarga>/det_DesPlanta}" renderMode="Narrow" colorScheme="8" />
                                        </HBox>

                                        <HBox class="sapUiTinyMarginBottom">
                                            <Label text="Embarcación:" class="sapUiTinyMarginEnd"/>
                                            <tnt:InfoLabel text="{popup_descarga>/det_Emba}" renderMode="Narrow" colorScheme="8" />
                                        </HBox>

                                        <HBox class="sapUiTinyMarginBottom">
                                            <Label text="Nom. embarcación:" class="sapUiTinyMarginEnd"/>
                                            <tnt:InfoLabel text="{popup_descarga>/det_DesEmba}" renderMode="Narrow" colorScheme="8" />
                                        </HBox>

                                        <HBox>
                                            <Label text="Especie:" class="sapUiTinyMarginEnd"/>
                                            <tnt:InfoLabel text="{popup_descarga>/det_Especie}" renderMode="Narrow" colorScheme="8" />
                                        </HBox>
                                    </VBox>

                                    <VBox class="sapUiSmallMarginEnd sapUiSmallMarginBottom">
                                        <HBox class="sapUiTinyMarginBottom">
                                            <Label text="Desc. especie:" class="sapUiSmallMarginEnd"/>
                                            <tnt:InfoLabel text="{popup_descarga>/det_DesEspecie}" renderMode="Narrow" colorScheme="8" />
                                        </HBox>

                                        <HBox class="sapUiTinyMarginBottom" renderType="Bare">
                                            <Label text="Lado:" class="sapUiSmallMarginEnd"/>
                                            <tnt:InfoLabel text="{popup_descarga>/det_Lado}" renderMode="Narrow" colorScheme="8" />
                                        </HBox>

                                        <HBox class="sapUiTinyMarginBottom">
                                            <Label text="Cantd. pesca:" class="sapUiSmallMarginEnd"/>
                                            <tnt:InfoLabel text="{popup_descarga>/det_CantPesca}" renderMode="Narrow" colorScheme="8" />
                                        </HBox>

                                        <HBox renderType="Bare">
                                            <Label text="Fecha/hora inicio:" class="sapUiSmallMarginEnd"/>
                                            <tnt:InfoLabel text="{popup_descarga>/det_FechaIni}" renderMode="Narrow" colorScheme="8" />
                                        </HBox>
                                    </VBox>

                                    <VBox class="sapUiSmallMarginEnd sapUiSmallMarginBottom">
                                        <HBox class="sapUiTinyMarginBottom">
                                            <Label text="Fecha/hora fin:" class="sapUiSmallMarginEnd"/>
                                            <tnt:InfoLabel text="{popup_descarga>/det_FechaFin}" renderMode="Narrow" colorScheme="8" />
                                        </HBox>

                                        <HBox class="sapUiTinyMarginBottom" renderType="Bare">
                                            <Label text="Desc. Estado:" class="sapUiSmallMarginEnd"/>
                                            <tnt:InfoLabel text="{popup_descarga>/det_Estado}" renderMode="Narrow" colorScheme="8" />
                                        </HBox>

                                        <HBox class="sapUiTinyMarginBottom">
                                            <Label text="Header:" class="sapUiSmallMarginEnd"/>
                                            <tnt:InfoLabel text="{popup_descarga>/det_Header}" renderMode="Narrow" colorScheme="8" />
                                        </HBox>

                                    </VBox>
                                </HeaderContainer>
                </content>
            </Panel>
